{
  "name": "streetball-tournament-app",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "npx expo start --web",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.1.0",
    "@react-native-firebase/analytics": "18.9.0",
    "@react-native-firebase/app": "18.9.0",
    "@react-native-firebase/auth": "18.9.0",
    "@react-native-firebase/firestore": "18.9.0",
    "@react-native-firebase/functions": "18.9.0",
    "@react-native-firebase/messaging": "18.9.0",
    "@react-native-firebase/storage": "18.9.0",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/stack": "^6.3.1",
    "axios": "^1.8.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.4",
    "expo": "~50.0.6",
    "expo-font": "~11.10.3",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "^0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "^0.24.13",
    "firebase": "^10.8.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.12",
    "@expo/metro-runtime": "~3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.5",
    "@types/react": "~18.2.0",
    "@types/react-native": "~0.73.0",
    "typescript": "^5.5.0"
  },
  "expo": {
    "name": "Streetball Tournament",
    "slug": "streetball-tournament",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.streetballtournament",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.yourcompany.streetballtournament",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/firestore",
      "@react-native-firebase/storage",
      "@react-native-firebase/functions",
      "@react-native-firebase/messaging",
      "@react-native-firebase/analytics"
    ]
  }
}
